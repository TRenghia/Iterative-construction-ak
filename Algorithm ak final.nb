Cell[TextData[{
 StyleBox["In the following script, we will set a non commutative algebra of \
operators. To properly deal with non commutative formal operators, we will \
need the NCAlgebra package (for download and further information, see the \
Link ",
  FontSize->14],
 StyleBox[ButtonBox["http://www.math.ucsd.edu/~ncalg/",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["http://www.math.ucsd.edu/~ncalg/"], None},
  ButtonNote->"http://www.math.ucsd.edu/~ncalg/"],
  FontSize->14],
 StyleBox[", Copyright (c) Helton and de Oliveira 2017,  All rights \
reserved). \n\nThe algorithm is developed as follows:\nSet ",
  FontSize->14],
 Cell[BoxData[
  RowBox[{"v", "=", 
   RowBox[{"(", 
    RowBox[{"I", ",", "\[CapitalLambda]"}], ")"}]}]], "Chapter",
  FontSize->14,ExpressionUUID->"2e8a795e-919b-4cbb-bb99-0e45d37dd9b2"],
 StyleBox[", then:\n",
  FontSize->14],
 Cell[BoxData[
  RowBox[{"Lv", "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"LI", ",", "L\[CapitalLambda]"}], ")"}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"I\[CapitalDelta]", "+", "\[CapitalLambda]N"}], " ", ",", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[PartialD]", "t"], "IN"}], "-", "I\[CapitalDelta]N", 
        "+", 
        RowBox[{"\[CapitalLambda]", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", 
            SuperscriptBox["N", "2"]}], "+", "\[CapitalDelta]"}], ")"}]}]}]}],
       ")"}], "=", 
     RowBox[{
      SubscriptBox["vM", "10"], "+", 
      RowBox[{
       SubscriptBox["\[PartialD]", "t"], 
       SubscriptBox["vM", "11"]}]}]}]}]}]], "Chapter",
  FontSize->14,ExpressionUUID->"122cf953-6bc8-4f4d-9e22-545cf6bbd906"],
 StyleBox["\nhaving set\n",
  FontSize->14],
 Cell[BoxData[
  TagBox[
   RowBox[{
    RowBox[{
     SubscriptBox["M", "10"], "=", 
     RowBox[{
      RowBox[{"(", "\[NoBreak]", GridBox[{
         {
          SubscriptBox["Q", "10"], 
          SubscriptBox["S", "10"]},
         {
          SubscriptBox["P", "10"], 
          SubscriptBox["R", "10"]}
        },
        GridBoxAlignment->{
         "Columns" -> {{Center}}, "ColumnsIndexed" -> {}, 
          "Rows" -> {{Baseline}}, "RowsIndexed" -> {}},
        GridBoxSpacings->{"Columns" -> {
            Offset[0.27999999999999997`], {
             Offset[0.7]}, 
            Offset[0.27999999999999997`]}, "ColumnsIndexed" -> {}, "Rows" -> {
            Offset[0.2], {
             Offset[0.4]}, 
            Offset[0.2]}, "RowsIndexed" -> {}}], "\[NoBreak]", ")"}], "=", 
      RowBox[{"(", "\[NoBreak]", GridBox[{
         {"\[CapitalDelta]", 
          RowBox[{
           RowBox[{"-", 
            FormBox["\[CapitalDelta]",
             TraditionalForm]}], "N"}]},
         {"N", 
          RowBox[{
           RowBox[{"-", 
            SuperscriptBox["N", "2"]}], "+", "\[CapitalDelta]"}]}
        },
        GridBoxAlignment->{
         "Columns" -> {{Center}}, "ColumnsIndexed" -> {}, 
          "Rows" -> {{Baseline}}, "RowsIndexed" -> {}},
        GridBoxSpacings->{"Columns" -> {
            Offset[0.27999999999999997`], {
             Offset[0.7]}, 
            Offset[0.27999999999999997`]}, "ColumnsIndexed" -> {}, "Rows" -> {
            Offset[0.2], {
             Offset[0.4]}, 
            Offset[0.2]}, "RowsIndexed" -> {}}], "\[NoBreak]", ")"}]}]}], ",",
     "\[IndentingNewLine]", 
    RowBox[{
     SubscriptBox["M", "11"], "=", 
     RowBox[{
      RowBox[{"(", "\[NoBreak]", GridBox[{
         {
          SubscriptBox["Q", "11"], 
          SubscriptBox["S", "11"]},
         {
          SubscriptBox["P", "11"], 
          SubscriptBox["R", "11"]}
        },
        GridBoxAlignment->{
         "Columns" -> {{Center}}, "ColumnsIndexed" -> {}, 
          "Rows" -> {{Baseline}}, "RowsIndexed" -> {}},
        GridBoxSpacings->{"Columns" -> {
            Offset[0.27999999999999997`], {
             Offset[0.7]}, 
            Offset[0.27999999999999997`]}, "ColumnsIndexed" -> {}, "Rows" -> {
            Offset[0.2], {
             Offset[0.4]}, 
            Offset[0.2]}, "RowsIndexed" -> {}}], "\[NoBreak]", ")"}], "=", 
      RowBox[{
       RowBox[{"(", "\[NoBreak]", GridBox[{
          {"0", "N"},
          {"0", "0"}
         },
         GridBoxAlignment->{
          "Columns" -> {{Center}}, "ColumnsIndexed" -> {}, 
           "Rows" -> {{Baseline}}, "RowsIndexed" -> {}},
         GridBoxSpacings->{"Columns" -> {
             Offset[0.27999999999999997`], {
              Offset[0.7]}, 
             Offset[0.27999999999999997`]}, "ColumnsIndexed" -> {}, "Rows" -> {
             Offset[0.2], {
              Offset[0.4]}, 
             Offset[0.2]}, "RowsIndexed" -> {}}], "\[NoBreak]", ")"}], 
       "."}]}]}]}],
   Function[BoxForm`e$, 
    MatrixForm[BoxForm`e$]]]], "Chapter",
  FontSize->14,ExpressionUUID->"3965be99-2e91-4f0b-a852-c9d23da2eb9c"],
 StyleBox["\nBy an induction argument, one can prove that \n",
  FontSize->14],
 Cell[BoxData[
  RowBox[{
   RowBox[{
    SuperscriptBox["L", "k"], "v"}], "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"j", "=", "0"}], "k"], 
       RowBox[{
        TagBox[
         FractionBox[
          SuperscriptBox["\[PartialD]", "j"], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["t", "j"]}]],
         DisplayForm], 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["IQ", "kj"], "+", 
          SubscriptBox["\[CapitalLambda]P", "kj"]}], ")"}]}]}], ",", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"j", "=", "0"}], "k"], 
       RowBox[{
        TagBox[
         FractionBox[
          SuperscriptBox["\[PartialD]", "j"], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["t", "j"]}]],
         DisplayForm], 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["IS", "kj"], "+", 
          SubscriptBox["\[CapitalLambda]R", "kj"]}], ")"}]}]}]}], ")"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"j", "=", "0"}], "k"], 
     RowBox[{
      RowBox[{
       TagBox[
        FractionBox[
         SuperscriptBox["\[PartialD]", "j"], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["t", "j"]}]],
        DisplayForm], "v"}], " ", 
      SubscriptBox["M", "kj"]}]}]}]}]], "Chapter",
  FontSize->14,ExpressionUUID->"417fff90-c717-4d81-b191-eb2345c3ad7d"],
 StyleBox["\nwhere the ",
  FontSize->14],
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"k", ",", "j"}], ")"}], TraditionalForm]], "Chapter",
  FontSize->14,ExpressionUUID->"5f484384-24d3-4f83-9e83-11f787bf7798"],
 StyleBox["-th matrix, for ",
  FontSize->14],
 Cell[BoxData[
  RowBox[{"0", "\[LessEqual]", " ", "j", "\[LessEqual]", "k"}]], "Chapter",
  FontSize->14,ExpressionUUID->"c2a68ff5-1304-4105-b3d2-a85da1f41af6"],
 StyleBox[", is defined by\n",
  FontSize->14],
 Cell[BoxData[
  RowBox[{
   SubscriptBox["M", "kj"], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["M", "10"], 
      SubscriptBox["M", 
       RowBox[{
        RowBox[{"k", "-", "1"}], ",", "j"}]]}], "+", 
     RowBox[{
      SubscriptBox["M", "11"], 
      SubscriptBox["M", 
       RowBox[{
        RowBox[{"k", "-", "1"}], ",", 
        RowBox[{"j", "-", "1"}]}]]}]}], "=", 
    RowBox[{"(", "\[NoBreak]", GridBox[{
       {
        SubscriptBox["Q", "kj"], 
        SubscriptBox["S", "kj"]},
       {
        SubscriptBox["P", "kj"], 
        SubscriptBox["R", "kj"]}
      },
      GridBoxAlignment->{
       "Columns" -> {{Center}}, "ColumnsIndexed" -> {}, 
        "Rows" -> {{Baseline}}, "RowsIndexed" -> {}},
      GridBoxSpacings->{"Columns" -> {
          Offset[0.27999999999999997`], {
           Offset[0.7]}, 
          Offset[0.27999999999999997`]}, "ColumnsIndexed" -> {}, "Rows" -> {
          Offset[0.2], {
           Offset[0.4]}, 
          Offset[0.2]}, "RowsIndexed" -> {}}], "\[NoBreak]", ")"}]}]}]], 
  "Chapter",
  FontSize->14,ExpressionUUID->"e8990df2-7f87-4270-94c5-f7a5ab8e77ee"],
 StyleBox[".\nHaving built the matrices ",
  FontSize->14],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["M", "kj"], "}"}], TraditionalForm]], "Chapter",
  FontSize->14,ExpressionUUID->"e2ff7f00-5fb8-403b-afa0-62fcab9954df"],
 StyleBox[", we can extract the families of operators ",
  FontSize->14],
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["S", "kj"], "}"}], TraditionalForm]], "Chapter",
  FontSize->14,ExpressionUUID->"b28e1fad-4138-405d-9245-cb4ffe64e72e"],
 StyleBox[",",
  FontSize->14],
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{"{", 
     SubscriptBox["R", "kj"], "}"}]}], TraditionalForm]], "Chapter",
  FontSize->14,ExpressionUUID->"3fe52683-1178-48f7-a6e2-3b3583e7b19b"],
 StyleBox[", which allow us to define the following:\n",
  FontSize->14],
 Cell[BoxData[{
  RowBox[{
   SubscriptBox["A", "k"], "=", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"j", "=", "0"}], 
     RowBox[{"k", "+", "1"}]], 
    RowBox[{
     SubscriptBox[
      RowBox[{"{", 
       RowBox[{"k", ",", "j"}], "}"}], "1"], 
     SubscriptBox["S", 
      RowBox[{
       RowBox[{"k", "+", "j", "-", "1"}], ",", 
       "j"}]]}]}]}], "\[IndentingNewLine]", 
  RowBox[{
   SubscriptBox["Z", "k"], "=", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"j", "=", "0"}], 
     RowBox[{"k", "-", "1"}]], 
    RowBox[{
     SubscriptBox[
      RowBox[{"{", 
       RowBox[{"k", ",", 
        RowBox[{"j", "-", "1"}]}], "}"}], "1"], 
     SubscriptBox["R", 
      RowBox[{
       RowBox[{"k", "+", "j", "-", "1"}], ",", "j"}]]}]}]}]}], "Chapter",
  FontSize->14,ExpressionUUID->"be3f14b8-0db5-4aa3-8846-5332a910c5a5"],
 StyleBox["\nwhere ",
  FontSize->14],
 Cell[BoxData[
  SubscriptBox[
   RowBox[{"{", 
    RowBox[{".", 
     RowBox[{",", "."}]}], "}"}], "1"]], "Chapter",
  FontSize->14,ExpressionUUID->"73f3ffd9-c360-46a5-93cd-24d44ddb9674"],
 StyleBox[" denotes, for any integer ",
  FontSize->14],
 Cell[BoxData[
  RowBox[{"k", ",", "j"}]], "Chapter",
  CellChangeTimes->{3.79707030996981*^9},
  FontSize->14,ExpressionUUID->"42633970-c50c-4858-a578-219d7b2af905"],
 ",",
 StyleBox[" the pairing\n",
  FontSize->14],
 Cell[BoxData[
  RowBox[{
   SubscriptBox[
    RowBox[{"{", 
     RowBox[{"k", ",", "j"}], "}"}], "1"], "=", 
   TagBox[
    FractionBox[
     RowBox[{"\[CapitalGamma]", 
      RowBox[{"(", 
       RowBox[{"k", "+", "j", "+", 
        RowBox[{"1", "/", "2"}]}], ")"}]}], 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"k", "+", "j"}], ")"}], "!"}], "\[CapitalGamma]", 
      RowBox[{"(", 
       RowBox[{"k", "+", 
        RowBox[{"1", "/", "2"}]}], ")"}]}]],
    DisplayForm]}]], "Chapter",
  FontSize->14,ExpressionUUID->"13ac2d90-ac8f-4e53-9cf9-40d0715fa8bc"],
 StyleBox[",\nhere ",
  FontSize->14],
 Cell[BoxData[
  TagBox[
   RowBox[{"\[CapitalGamma]", 
    RowBox[{"(", ".", ")"}]}],
   DisplayForm]], "Chapter",
  FontSize->14,ExpressionUUID->"27ea43a5-cdcb-4cd5-b7ee-a7d20f6fec9c"],
 StyleBox[" is the Euler Gamma function. \nFinally, we are able to define the \
operators ",
  FontSize->14],
 Cell[BoxData[
  SubscriptBox["D", "k"]], "Chapter",
  CellChangeTimes->{3.79707030996981*^9},
  FontSize->14,ExpressionUUID->"49f530d0-9f37-4ccf-bd3b-db959d05148c"],
 StyleBox[", by induction:\n",
  FontSize->14],
 Cell[BoxData[{
  RowBox[{
   SubscriptBox["D", "1"], "=", 
   RowBox[{
    FractionBox["2", 
     SqrtBox["\[Pi]"]], "Id"}]}], "\[IndentingNewLine]", 
  RowBox[{
   TagBox[
    RowBox[{
     SubscriptBox["D", 
      RowBox[{"2", "n"}]], "=", 
     FractionBox["1", 
      SqrtBox["\[Pi]"]]}],
    DisplayForm], 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"i", "=", "0"}], "n"], 
    RowBox[{
     SubscriptBox[
      RowBox[{"{", 
       RowBox[{"n", ",", "i"}], "}"}], "2"], 
     SubscriptBox["D", 
      RowBox[{
       RowBox[{"2", "i"}], "-", "1"}]], 
     SubscriptBox["A", 
      RowBox[{"n", "-", "i"}]]}]}]}], "\[IndentingNewLine]", 
  RowBox[{
   TagBox[
    RowBox[{
     SubscriptBox["D", 
      RowBox[{
       RowBox[{"2", "n"}], "+", "1"}]], "=", 
     RowBox[{
      RowBox[{
       FractionBox["1", 
        SqrtBox["\[Pi]"]], 
       SubscriptBox["Z", 
        RowBox[{"n", "+", "1"}]]}], "+", 
      FractionBox["1", 
       SqrtBox["\[Pi]"]]}]}],
    DisplayForm], 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"i", "=", "1"}], "n"], 
    RowBox[{
     SubscriptBox[
      RowBox[{"{", 
       RowBox[{"n", ",", "i"}], "}"}], "3"], 
     SubscriptBox["D", 
      RowBox[{"2", "i"}]], 
     SubscriptBox["A", 
      RowBox[{"n", "-", "i"}]]}]}]}]}], "Chapter",
  CellChangeTimes->{3.79707030996981*^9},
  FontSize->14,ExpressionUUID->"98ab3c64-1641-4e26-95f7-b49036d9fb8b"],
 StyleBox["\nwhere ",
  FontSize->14],
 Cell[BoxData[
  SubscriptBox[
   RowBox[{"{", 
    RowBox[{".", 
     RowBox[{",", "."}]}], "}"}], "2"]], "Chapter",
  FontSize->14,ExpressionUUID->"9b74c15b-6749-4a30-93aa-bd45d4e9210f"],
 StyleBox[" and ",
  FontSize->14],
 Cell[BoxData[
  SubscriptBox[
   RowBox[{"{", 
    RowBox[{".", 
     RowBox[{",", "."}]}], "}"}], "3"]], "Chapter",
  FontSize->14,ExpressionUUID->"1fe1fa3a-49b9-40b3-b257-fc0802fd9531"],
 StyleBox[" denote, for any integer ",
  FontSize->14],
 Cell[BoxData[
  RowBox[{"k", ",", "j"}]], "Chapter",
  CellChangeTimes->{3.79707030996981*^9},
  FontSize->14,ExpressionUUID->"bbb10229-123a-4640-853a-231e33c08f45"],
 StyleBox[", respectively the pairing\n",
  FontSize->14],
 Cell[BoxData[
  RowBox[{
   SubscriptBox[
    RowBox[{"{", 
     RowBox[{"k", ",", "j"}], "}"}], "2"], "=", 
